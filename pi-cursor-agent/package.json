{
  "name": "pi-cursor-agent",
  "version": "0.4.0",
  "description": "Cursor Agent provider extension for pi",
  "type": "module",
  "license": "MIT",
  "author": "sudosubin",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sudosubin/pi-frontier.git",
    "directory": "pi-cursor-agent"
  },
  "keywords": [
    "pi",
    "cursor",
    "cursor-agent",
    "cursor-cli",
    "ai",
    "llm"
  ],
  "pi": {
    "extensions": [
      "./src/index.ts"
    ]
  },
  "files": [
    "src/**/*.ts",
    "README.md"
  ],
  "scripts": {
    "lint": "biome check",
    "format": "biome check --write",
    "typecheck": "tsc",
    "typecheck:watch": "tsc --watch",
    "proto:generate": "buf generate"
  },
  "dependencies": {
    "@bufbuild/protobuf": "1.10.0",
    "@connectrpc/connect": "^1.7.0",
    "@connectrpc/connect-node": "^1.7.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.4.2",
    "@bufbuild/buf": "^1.65.0",
    "@bufbuild/protoc-gen-es": "1.10.0",
    "@connectrpc/protoc-gen-connect-es": "^1.7.0",
    "@tsconfig/strictest": "^2.0.8",
    "@types/node": "^24.10.10",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@mariozechner/pi-ai": ">=0.49.0",
    "@mariozechner/pi-coding-agent": ">=0.49.0"
  }
}
